{
  "name": "green-jade-arcade-lobby-v2",
  "version": "2.5.8",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint ./src",
    "lint:fix": "next lint --fix",
    "test": "npm run update-version & npm run lint",
    "prepare": "husky install",
    "ui:dev": "npx ladle serve",
    "ui:build": "npx ladle build -o ./docs/ui",
    "ui:preview": "npx ladle preview -o ./docs/ui",
    "cypress": "cypress open",
    "update-version": "npm version patch -git-tag-version false && git add package.json && git add package-lock.json"
  },
  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@stitches/react": "^1.2.8",
    "next": "^13.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "^6.0.0-beta.5",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@ladle/react": "^2.4.5",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/styled-components": "^5.1.26",
    "@types/styled-system": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "cypress": "^10.11.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^13.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "4.8.4"
  }
}
